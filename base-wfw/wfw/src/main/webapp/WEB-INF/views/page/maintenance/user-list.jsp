
<div layout:fragment="content">
  <div id="content-home">
  <div id="search_criteria">
  <form th:action="@{/maintenance/user-edit}" method="post">
	  <input class="btnAddNew" th:type="submit" th:value="#{btn.cmn.create.new.usr}" th:name="create" />
  </form>
  </div>
  
     <div id="data_table">
		<table id="tbl_data">
		   <tr>
			<th colspan="8" th:text="#{lbl.users}"></th>
		  </tr>
		  <tr>
	        <th>Name</th>
	        <th>User Name</th>
	        <th>Password</th>
	        <th>User Role</th>
	        <th>Gender</th>
	        <th>Active</th>
	        <th>Action</th>
	      </tr>
	      <tr th:each="user : ${dataListObj}">
	      	<td th:text="${user.name}"></td>
	        <td th:text="${user.userName}"></td>
	        <td th:text="${user.password}"></td>
	        <td th:text="${user.userRole}"></td>
	        <td th:text="${user.sex} == M ? #{Male} : #{Female}"></td>
	        <td th:text="${user.isActive} == 1 ? #{Yes} : #{No}"></td>
	        <td style="text-align: center;"><a th:href="@{/maintenance/user-edit(userId=${user.wfUserId})}"><span>Edit</span></a></td>
	      </tr>
	    </table>
	 </div>
   </div>
</div>